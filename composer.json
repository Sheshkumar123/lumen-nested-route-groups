{
  "name": "fremail/lumen-nested-route-groups",
  "type": "library",
  "version": "1.1.0",
  "description": "Extends a lumen application for using nested route groups. In addition to the groups it's possible to use any() and match() methods.",
  "keywords": ["lumen", "route", "group", "any", "match"],
  "license": "MIT",
  "authors": [
    {
      "name": "Anton Demushkin",
      "email": "fremail@yandex.com"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "laravel/lumen-framework": "5.*"
  },
  "autoload": {
    "psr-4": {
      "Fremail\\NestedRouteGroups\\": "src"
    }
  },
  "config":{
    "preferred-install": "dist"
  }
}
